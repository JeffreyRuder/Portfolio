
{{#paper-nav-container}}
  {{#paper-sidenav class="md-sidenav-left md-whiteframe-z2" locked-open="gt-md"}}
    {{#paper-toolbar}}
      <div class="md-toolbar-tools">
        <div class="brand">
          {{#link-to 'index'}}<strong>Jeffrey Ruder</strong>{{/link-to}}
        </div>
      </div>
    {{/paper-toolbar}}

    {{#paper-content}}
      {{#paper-list}}
        {{#paper-item}}{{#scroll-to href="#about_heading"}}About{{/scroll-to}}{{/paper-item}}
        {{#paper-item}}{{#scroll-to href="#projects_heading"}}Projects{{/scroll-to}}{{/paper-item}}
        {{#paper-item}}{{#scroll-to href="#education_heading"}}Education{{/scroll-to}}{{/paper-item}}
        {{#paper-item}}{{#scroll-to href="#contact_heading"}}Contact{{/scroll-to}}{{/paper-item}}
      {{/paper-list}}
    {{/paper-content}}
  {{/paper-sidenav}}

  <div layout="column" tabindex="-1" role="main" flex>
    {{#paper-toolbar}}
      <div class="md-toolbar-tools">
        {{#paper-sidenav-toggle as |toggle|}}
          {{#paper-button target=toggle action="toggleMenu" icon-button=true}}{{paper-icon "menu"}}{{/paper-button}}
        {{/paper-sidenav-toggle}}
        <span class="md-breadcrumb-page">

        </span>
      </div>
    {{/paper-toolbar}}

    {{#paper-content class="md-padding main-content"}}
      {{outlet}}
    {{/paper-content}}
  </div>
{{/paper-nav-container}}
