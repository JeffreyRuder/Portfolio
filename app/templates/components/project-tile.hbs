{{#if skillsInCommon}}
  {{#paper-card}}
    <div class="project-image" style={{cleanCSS project.image}}></div>
    {{#paper-card-content}}
      <h2>{{project.name}}</h2>
      <p>
        {{project.description}}
      </p>
      <div class="md-actions" layout="row" layout-align="end center">
        {{#each project.skills as |skill|}}
          <span class="project-skill-name">{{skill.name}}</span>
        {{/each}}
        <a href={{project.github_url}} target="_blank">{{#paper-button}}GitHub{{/paper-button}}</a>
        {{#if project.deployment_url}}
          <a href={{project.deployment_url}} target="_blank">{{#paper-button}}Demo{{/paper-button}}</a>
        {{/if}}
    </div>
    {{/paper-card-content}}
  {{/paper-card}}
{{/if}}
